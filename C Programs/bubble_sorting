#include <stdio.h>

int main()
{
  int n,arr[10],i,j,temp;
  printf("enter the value for array :");
  scanf("%d",&n);
  printf("enter the %d value for array",n);
  for(i=0;i<n;i++)
  {
      scanf("%d",&arr[i]);
  }
  for(i=0;i<n-1;i++)
  {
     for(j=0;j<n-1-i;j++)
     {
         if(arr[j]>arr[j+1])
         {
             temp=arr[j];
             arr[j]=arr[j+1];
             arr[j+1]=temp;
         }
     }
  }
   
      printf("Sorted elements for Bubble sort: ");
   for(i=0;i<n;i++)
      printf(" %d",arr[i]);

   return 0; 
}
