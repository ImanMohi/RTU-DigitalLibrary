#include<iostream>
int kadane(int arr[],int n){
    int csum=0;
    int max=0;
    for(int i=0;i<n;i++){
        csum =csum + arr[i];
        if(max<csum){
            max=csum;
        }
        
        if(csum<0)
        {
            csum=0;
        }
      
    }
    return max;
}
using namespace std;
int main(){
    //int t;
    //cin>>t;
    //while(t--){
        int n,k;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++){
            cin>>arr[i];
        }
        cout<<kadane(arr,n)<<endl;
   // }
    return 0;
}
